  "assessment_questions": [
    {
      "id": "7d783768-f6a4-4a78-8e16-ba75bf017371-q-1",
      "type": "single_choice",
      "question": "What is the main purpose of cell division?",
      "options": [
        "A. To produce energy",
        "B. To stop cell growth",
        "C. To create new cells for growth, repair, and reproduction",
        "D. To eliminate old cells"
      ],
      "correct_answer": "C. To create new cells for growth, repair, and reproduction",
      "points": 1
    },
    {
      "id": "7d783768-f6a4-4a78-8e16-ba75bf017371-q-2",
      "type": "single_choice",
      "question": "Which type of cell division results in genetically identical daughter cells?",
      "options": [
        "A. Mitosis",
        "B. Fertilization",
        "C. Binary fission",
        "D. Meiosis"
      ],
      "correct_answer": "A. Mitosis",
      "points": 1
    },
    {
      "id": "7d783768-f6a4-4a78-8e16-ba75bf017371-q-3",
      "type": "single_choice",
      "question": "What phase of the cell cycle involves DNA replication?",
      "options": [
        "A. M Phase",
        "B. G2 Phase",
        "C. S Phase",
        "D. G1 Phase"
      ],
      "correct_answer": "C. S Phase",
      "points": 1
    },
    {
      "id": "7d783768-f6a4-4a78-8e16-ba75bf017371-q-4",
      "type": "single_choice",
      "question": "How many daughter cells are produced in meiosis?",
      "options": [
        "A. 2",
        "B. 8",
        "C. 4",
        "D. 6"
      ],
      "correct_answer": "C. 4",
      "points": 1
