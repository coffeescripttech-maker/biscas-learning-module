      {
        "id": "001fae52-9758-4063-a2ea-8b05c811d4e6",
        "title": "Post-Test",
        "content_type": "assessment",
        "content_data": {
          "questions": [
            {
              "id": "001fae52-9758-4063-a2ea-8b05c811d4e6-q-1",
              "type": "single_choice",
              "question": "What is the chromosome number in human gametes produced by meiosis?",
              "options": [
                "A. 23",
                "B. 92",
                "C. 46",
                "D. 12"
              ],
              "correct_answer": "A. 23",
              "points": 1
            },
            {
              "id": "001fae52-9758-4063-a2ea-8b05c811d4e6-q-2",
              "type": "single_choice",
              "question": "Which phase of meiosis involves homologous chromosomes separating?",
              "options": [
                "A. Metaphase II",
                "B. Anaphase I",
                "C. Telophase II",
                "D. Prophase I"
              ],
              "correct_answer": "B. Anaphase I",
              "points": 1
            },
            {
              "id": "001fae52-9758-4063-a2ea-8b05c811d4e6-q-3",
              "type": "single_choice",
              "question": "What structure forms during synapsis in Prophase I?",
              "options": [
                "A. Spindle fiber",
                "B. Chromatid",
                "C. Tetrad",
                "D. Centromere"
              ],
              "correct_answer": "C. Tetrad",
              "points": 1
            },
            {
              "id": "001fae52-9758-4063-a2ea-8b05c811d4e6-q-4",
              "type": "single_choice",
              "question": "What is the result of fertilization in humans?",
              "options": [
                "A. Four gametes",
                "B. A diploid zygote",
                "C. Two haploid cells",
                "D. A single sperm cell"
              ],
              "correct_answer": "B. A diploid zygote",
              "points": 1
            },
            {
              "id": "001fae52-9758-4063-a2ea-8b05c811d4e6-q-5",
              "type": "single_choice",
              "question": "Which phase of the cell cycle prepares the cell for division by checking DNA and organizing chromosomes?",
              "options": [
                "A. M Phase",
                "B. G2 Phase",
                "C. G1 Phase",
                "D. S Phase"
              ],
              "correct_answer": "B. G2 Phase",
              "points": 1
            },
            {
              "id": "001fae52-9758-4063-a2ea-8b05c811d4e6-q-6",
              "type": "single_choice",
              "question": "What happens during Metaphase II of meiosis?",
              "options": [
                "A. DNA replication occurs",
                "B. Sister chromatids separate",
                "C. Chromosomes line up individually along the equatorial plate",
                "D. Homologous chromosomes pair up"
              ],
              "correct_answer": "C. Chromosomes line up individually along the equatorial plate",
              "points": 1
            },
            {
              "id": "001fae52-9758-4063-a2ea-8b05c811d4e6-q-7",
              "type": "single_choice",
              "question": "Which organ produces sperm cells through meiosis?",
              "options": [
                "A. Ovary",
                "B. Brain",
                "C. Testes",
                "D. Liver"
              ],
              "correct_answer": "C. Testes",
              "points": 1
            },
            {
              "id": "001fae52-9758-4063-a2ea-8b05c811d4e6-q-8",
              "type": "single_choice",
              "question": "What is the role of mitochondria in sperm cells?",
              "options": [
                "A. Provide energy for movement",
                "B. Store DNA",
                "C. Help fertilization",
                "D. Form the tail"
              ],
              "correct_answer": "A. Provide energy for movement",
              "points": 1
            },
            {
              "id": "001fae52-9758-4063-a2ea-8b05c811d4e6-q-9",
              "type": "single_choice",
              "question": "What happens to the tiny cells produced during meiosis in females?",
              "options": [
                "A. They fertilize the egg",
                "B. They develop into embryos",
                "C. They become sperm cells",
                "D. They break down"
              ],
              "correct_answer": "D. They break down",
              "points": 1
            },
            {
              "id": "001fae52-9758-4063-a2ea-8b05c811d4e6-q-10",
              "type": "single_choice",
              "question": "Why is meiosis important for sexual reproduction?",
              "options": [
                "A. It creates genetically unique gametes",
                "B. It produces identical cells",
                "C. It repairs damaged tissues",
                "D. It increases chromosome number"
              ],
              "correct_answer": "A. It creates genetically unique gametes",
              "points": 1
            }
          ],
          "quiz_data": {
            "question": "Post-Test (Instruction: Click the letter of the correct answer for each question.)"
          }
        },
        "position": 6,
        "is_required": true,
        "time_estimate_minutes": 5,
