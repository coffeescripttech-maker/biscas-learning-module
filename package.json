{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "node -e \"const fs = require('fs'); const path = '.next'; if (fs.existsSync(path)) fs.rmSync(path, {recursive: true, force: true});\"",
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "verify-auth": "node scripts/verify-auth-setup.js",
    "setup-pwa": "node scripts/setup-pwa.js",
    "pwa-audit": "npx lighthouse http://localhost:3000 --only-categories=pwa --output=html --output-path=./lighthouse-pwa-report.html",
    "generate-icons": "node scripts/generate-icons.js",
    "cap:build": "npm run build && npx cap sync",
    "cap:android": "npm run cap:build && npx cap open android",
    "cap:ios": "npm run cap:build && npx cap open ios",
    "cap:serve": "npm run cap:build && npx cap run android",
    "cap:sync": "npx cap sync",
    "mobile:dev": "npm run build && npx cap sync && npx cap run android --livereload --external",
    "mobile:build:android": "npm run build && npx cap sync && cd android && gradlew assembleDebug",
    "mobile:build:ios": "npm run build && npx cap sync && npx cap build ios",
    "build:capacitor": "node scripts/build-capacitor.js",
    "apk:build": "npm run build:capacitor && cd android && ./gradlew assembleDebug",
    "apk:release": "npm run build:capacitor && cd android && ./gradlew assembleRelease"
  },
